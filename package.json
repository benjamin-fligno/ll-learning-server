{
  "name": "ll-learning-server",
  "version": "1.0.0",
  "description": "An expressjs server for restful api of ll-learning app",
  "main": "dist/app.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpileOnly ./src/app.ts",
    "build": "rm -rf ./dist && tsc",
    "serve": "node dist/app.js",
    "tsc": "tsc -- --init"
  },
  "keywords": [
    "express",
    "typescript",
    "restful-api",
    "node",
    "server"
  ],
  "author": "espanola.benjamin@gmail.com",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.2",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.7.1",
    "@types/socket.io": "^2.1.4",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  }
}
